package main

import "fmt"

type Rectangle struct {
	length		int
	width		int
}

func NewRectangle(length, width int) *Rectangle {
	return &Rectangle{
		length: length,
		width:  width
	}
}


func (r *Rectangle) Area() int {
	return r.length * r.width
}

func main() {
	r := NewRectangle(2, 3)
	fmt.Println(r.Area())
}
