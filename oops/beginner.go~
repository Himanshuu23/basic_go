package main

import "fmt"

type Car struct {
	model	string
	year 	string
}

func main() {
	A := Car{"a", "2020"}
	fmt.Println(A)
	A.model = "b"
	fmt.Println(A)
	Print(A)
	newCar := struct {
		model string
		year  string
	}{
		model "something",
		year  "something-else"
	}

	Print(newCar)
}

func Print(car Car) {
	fmt.Println(car.model, car.year)
}
